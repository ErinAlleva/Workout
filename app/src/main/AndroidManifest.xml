<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comeaa.workout">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OpenWorkout" />
        android:parentActivityName=".MainActivity" >
        <!-- The meta-data tag is required if you support API level 15 and lower -->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />

        <activity android:name=".Chest" />
        android:parentActivityName=".OpenWorkout" >
        <activity android:name=".Shoulders" />
        android:parentActivityName=".OpenWorkout" >
        <activity android:name=".Back" />
        android:parentActivityName=".OpenWorkout" >
        <activity android:name=".FullBody" />
        android:parentActivityName=".OpenWorkout" >
        <activity android:name=".BicepsTriceps" />
        android:parentActivityName=".OpenWorkout" >
        <activity android:name=".Legs">
            android:parentActivityName=".OpenWorkout" >
        </activity>
        <activity
            android:name=".bicepsDB"
            android:label="Biceps and Triceps Exercise Database"
            android:theme="@style/AppTheme.NoActionBar">
            android:parentActivityName=".BicepsTriceps" >
        </activity>
    </application>

</manifest>