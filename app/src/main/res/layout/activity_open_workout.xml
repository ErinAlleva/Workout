<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tableRow1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    app:layout_collapseParallaxMultiplier="0.7"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">


    <Button
        android:id="@+id/button9"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:onClick="FullBodyClick"
        android:text="Full Body"
        tools:layout_conversion_absoluteHeight="130dp"
        tools:layout_conversion_absoluteWidth="130dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/button12" />

    <Button
        android:id="@+id/button10"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:onClick="LegsClick"
        android:text="Legs"
        tools:layout_conversion_absoluteHeight="130dp"
        tools:layout_conversion_absoluteWidth="130dp"
        tools:layout_conversion_absoluteX="130dp"
        tools:layout_conversion_absoluteY="0dp"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        app:layout_constraintVertical_bias="0.698" />


    <Button
        android:id="@+id/button8"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:onClick="ShouldersClick"
        android:text="Shoulders"
        tools:layout_conversion_absoluteHeight="130dp"
        tools:layout_conversion_absoluteWidth="130dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="130dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button3"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:onClick="ChestClick"
        android:text="Chest"
        tools:layout_conversion_absoluteHeight="130dp"
        tools:layout_conversion_absoluteWidth="130dp"
        tools:layout_conversion_absoluteX="130dp"
        tools:layout_conversion_absoluteY="130dp"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="65dp"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintVertical_bias="0.0" />


    <Button
        android:id="@+id/button12"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:onClick="BackClick"
        android:text="Back"
        tools:layout_conversion_absoluteHeight="130dp"
        tools:layout_conversion_absoluteWidth="130dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="260dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toTopOf="@+id/button11" />

    <Button
        android:id="@+id/button11"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="32dp"
        android:onClick="BicepsTricepsClick"
        android:text="Biceps/Tris"
        app:layout_constraintBaseline_toBaselineOf="@+id/button10"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_conversion_absoluteHeight="130dp"
        tools:layout_conversion_absoluteWidth="130dp"
        tools:layout_conversion_absoluteX="130dp"
        tools:layout_conversion_absoluteY="268dp" />

</android.support.constraint.ConstraintLayout>
